{
  "name": "nodejstemplate",
  "version": "1.0.0",
  "description": "Nodejs project template for common directory structure and basic libraries & required files",
  "scripts": {
    "start": "nodemon",
    "start:dev": "cross-env NODE_ENV=development ts-node src/server.ts",
    "copy": "cp package.json build/",
    "build": "rimraf ./build && tsc && npm run copy",
    "start:prod": "npm run build && NODE_ENV=production node build/server.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts' --timeout 10000"
  },
  "keywords": [],
  "author": "Rohan",
  "license": "ISC",
  "main": "server.js",
  "dependencies": {
    "aws-sdk": "^2.1373.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.1",
    "express-useragent": "^1.0.15",
    "getmac": "^5.20.0",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "https": "^1.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.1",
    "mssql": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.2.9",
    "nodemailer": "^6.7.7",
    "otp-generator": "^4.0.1",
    "pg": "^8.7.3",
    "puppeteer": "^20.3.0",
    "save": "^2.9.0",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.1",
    "@types/jsonwebtoken": "^9.0.7",
